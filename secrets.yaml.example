# WiFi credentials
wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# Fallback AP password (used when WiFi connection fails)
ap_password: "YOUR_FALLBACK_AP_PASSWORD"

# API encryption (generate with: python3 -c "import secrets; import base64; print(base64.b64encode(secrets.token_bytes(32)).decode())")
encryption_key: "YOUR_32_CHAR_API_ENCRYPTION_KEY_HERE_123456"

# OTA password
ota_password: "YOUR_OTA_PASSWORD"

# MQTT broker
mqtt_broker: "YOUR_MQTT_BROKER_IP_OR_HOSTNAME"
